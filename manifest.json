{
    "manifest_version": 3,
    "name": "IroInfo",
    "version": "1.0",
    "description": "Chrome extension that picks on screen color and display related information.",
    "permissions": [
      "activeTab",
      "scripting",
      "tabs",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "IroInfo"
    }
  }
  